// toggle visibility of all layers named "boundingBox" (cmd l)
#import 'lib.js'

var searchPattern = "boundingBox"; 

var matchingLayers = getLayersWithName(searchPattern, [doc currentPage])

for (var j = 0; j < matchingLayers.length; j++) { 
   var child = matchingLayers[j]; 
  [child setIsVisible:![child isVisible]] 
}